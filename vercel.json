{
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.0.0",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/",
      "destination": "/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, stale-while-revalidate=7200"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' vitals.vercel-insights.com; img-src 'self' data:; style-src 'unsafe-inline'; connect-src 'self' vitals.vercel-insights.com"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}